####### Build rules

all: Makefile ../../bin/$(TARGET) 

staticlib: ../../bin/$(TARGET)

../../bin/$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC) $(OBJCOMP) 
	test -d ../../bin/ || mkdir -p ../../bin/
	-$(DEL_FILE) $(TARGET)
	$(AR) $(TARGET) $(OBJECTS) $(OBJMOC)
	-$(DEL_FILE) ../../bin/$(TARGET)
	-$(MOVE) $(TARGET) ../../bin/


mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: cppunit.pro  /usr/qt/3/mkspecs/linux-g++/qmake.conf 
	$(QMAKE) -o Makefile cppunit.pro
qmake: 
	@$(QMAKE) -o Makefile cppunit.pro

dist: 
	@mkdir -p obj/cppunit && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) obj/cppunit/ && ( cd `dirname obj/cppunit` && $(TAR) cppunit.tar cppunit && $(GZIP) cppunit.tar ) && $(MOVE) `dirname obj/cppunit`/cppunit.tar.gz . && $(DEL_FILE) -r obj/cppunit

mocclean:

uiclean:

yaccclean:
lexclean:
clean:
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


